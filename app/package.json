{
  "name": "liandi",
  "version": "0.1.2",
  "description": "链滴笔记",
  "main": "./electron/main.js",
  "scripts": {
    "lint": "tslint --fix -c tslint.json 'src/**/*.ts'",
    "dev": "webpack",
    "build": "webpack --mode production --config webpack.css.config.js && webpack --mode production",
    "start": "cross-env NODE_ENV=development electron ./electron/main.js",
    "pack": "cross-env ELECTRON_MIRROR=http://npm.taobao.org/mirrors/electron/ electron-builder --dir",
    "dist": "cross-env ELECTRON_MIRROR=http://npm.taobao.org/mirrors/electron/ electron-builder"
  },
  "keywords": [
    "markdown",
    "note-taking",
    "notebook",
    "webdav",
    "b3log"
  ],
  "author": "Vanessa <v@b3log.org> (http://vanessa.b3log.org)",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "electron": "^7.1.10",
    "electron-builder": "^22.3.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "vditor": "^2.2.18"
  }
}
